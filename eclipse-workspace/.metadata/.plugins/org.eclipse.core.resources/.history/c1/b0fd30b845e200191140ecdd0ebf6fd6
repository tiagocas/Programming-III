package guiao2;
import guiao1.Date;

public class Staff extends Person{
	private int finances;
	
	public Staff(String name, int cc, Date birthday, int finances){
		super(name, cc, birthday);
		this.finances = finances;
	}
	@Override
	public String toString() {
		return super.toString() + "\t -> " + this.finances;
	}
}
