package guiao2;
import java.util.HashMap;
import guiao1.Date;

public class LoansList {
	private HashMap<Integer, Loan> ll;
	private int id;
	
	public LoansList() {
		ll = new HashMap<Integer, Loan>();
		this.id = 0;
	}
	
	public boolean add(Video video, Person person, Date checkIn, Date checkOut) {
		ll.put(this.id, new Load(this.id ,video, person,checkIn, checkOut));
		this.id++;
	}

}
