package guiao2;
import guiao1.Date;

public class Person {
	private String name;
	private int cc;
	private Date birthday;
	private int id;
	//private Date registrationDate;
	
	Person(String name, int cc, Date birthday){
		this.name = name;
		this.cc = cc;
		this.birthday = birthday;
	}
	public void add(String name, int cc, Date birthday) {
		this.name = name;
		this.cc = cc;
		this.birthday
		
		this.id++;
	}
	
	Person(String name, String cc, Date birthday){
		this.name = name;
		this.cc = Integer.parseInt(cc);
		this.birthday = birthday;
	}
	
	public int id() {
		return this.id;
	}
	
	public String toString() {
		return this.cc + "\t" + this.name + "\t" + this.cc + "\t" + this.birthday;
	}
	
	
}
